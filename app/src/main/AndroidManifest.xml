<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.myprojects.numerus.spark_000.numerus"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".NavigationActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ThemesActivity"
            android:label="@string/title_activity_themes">
        </activity>
        <activity
            android:name=".CaesarCipherActivity"
            android:label="@string/title_activity_caesar_cipher"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".Rot13Activity"
            android:label="@string/title_activity_rot13"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".VigenereActivity"
            android:label="@string/title_activity_vigenere"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".ColumnTransActivity"
            android:label="@string/title_activity_column_trans"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>